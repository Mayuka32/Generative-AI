      "cell_type": "code",
      "source": [
        "from IPython import get_ipython\n",
        "from IPython.display import display\n",
        "# %%\n",
        "!pip install gradio\n",
        "!pip install gradio transformers torch\n",
        "import gradio as gr\n",
        "# Import MarianMTModel and MarianTokenizer from the correct submodule\n",
        "from transformers.models.marian import MarianMTModel, MarianTokenizer\n",
        "\n",
        "# Define translation model - you can change to other language pairs\n",
        "model_name = \"Helsinki-NLP/opus-mt-en-de\"  # English to German\n",
        "tokenizer = MarianTokenizer.from_pretrained(model_name)\n",
        "model = MarianMTModel.from_pretrained(model_name)\n",
        "\n",
        "# Translation function\n",
        "def translate_text(text):\n",
        "    if not text.strip():\n",
        "        return \"Please enter some text to translate.\"\n",
        "    inputs = tokenizer(text, return_tensors=\"pt\", padding=True)\n",
        "    translated = model.generate(**inputs)\n",
        "    result = tokenizer.decode(translated[0], skip_special_tokens=True)\n",
        "    return result\n",
        "\n",
        "# Gradio interface\n",
        "iface = gr.Interface(\n",
        "    fn=translate_text,\n",
        "    inputs=gr.Textbox(lines=5, placeholder=\"Enter English text here...\"),\n",
        "    outputs=\"text\",\n",
        "    title=\"AI Translator\",\n",
        "    description=\"Translate English text into German using a pretrained MarianMT model.\",\n",
        "    theme=\"default\"\n",
        ")\n",
        "\n",
        "# Launch app\n",
        "if __name__ == \"__main__\":\n",
        "    iface.launch()"
      ],
